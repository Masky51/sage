  "main_section": "<div class=\"ql-editor read-mode\"><p><br></p></div>",
  "main_section_html": "<h4>Student List</h4>\n<hr>\n<a href=\"http://learn.com:8013/add-stud\" class=\"btn btn-primary\">Add Student</a>\n<table class=\"table table-striped\">\n  <thead>\n    <tr>\n      <th scope=\"col\"></th>\n      <th scope=\"col\">Student ID</th>\n      <th scope=\"col\">First Name</th>\n      <th scope=\"col\">Last Name</th>\n      <th scope=\"col\">Gender</th>\n    </tr>\n  </thead>\n  <tbody>\n    {% for student in students %}\n    <tr>\n      <th><img class=\"avatar\" src=\"{{ student.profile_img }}\"></th>\n      <td>{{ student.name }}</td>\n      <td>{{ student.first_name }}</td>\n      <td>{{ student.last_name }}</td>\n      <td>{{ student.gender }}</td>\n    </tr>\n    {% endfor %}\n  </tbody>\n</table>",
